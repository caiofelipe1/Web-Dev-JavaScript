var aluno1 = {
    rm: 551555,
    nome: 'ricardo di tilia',
    curso:'eng soft',
    estadoCivil: 'oracle',
    ativo: true
};

console.log(aluno1.ra);
console.log(aluno1.nome);
console.log(aluno1.curso);
console.log(aluno1.ativo);

//Por ser dinamico, podemos criar novos campos ou excluir campos quando quiser 

aluno1.datanasc = '17/12/2005';

console.log(aluno1.datanasc);

delete aluno1.ativo;

